<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.doudou.bill.orm.dao.BookDao">
 
	<select id="bookList" resultType="com.doudou.bill.orm.model.Book">
		select * from m_book
	</select>

	<select id="getById" resultType="com.doudou.bill.orm.model.Book">
		select * from m_book WHERE id=#{id}
	</select>
</mapper>