<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.doudou.bill.orm.dao.UserDao">
 


  <select id="getUserById" resultType="com.doudou.bill.orm.model.User" parameterType="java.lang.String">
   	select 
   	*
   	from user
   	where
    user_id = #{UserId}
  </select>

</mapper>